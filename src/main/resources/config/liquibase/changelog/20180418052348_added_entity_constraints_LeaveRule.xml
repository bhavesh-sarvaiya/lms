<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity LeaveRule.
    -->
    <changeSet id="20180418052348-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="leave_rules_id"
                                 baseTableName="leave_rule_leave_type"
                                 constraintName="fk_leave_rule_leave_type_leave_rules_id"
                                 referencedColumnNames="id"
                                 referencedTableName="leave_rule"/>
        <addForeignKeyConstraint baseColumnNames="leave_types_id"
                                 baseTableName="leave_rule_leave_type"
                                 constraintName="fk_leave_rule_leave_type_leave_types_id"
                                 referencedColumnNames="id"
                                 referencedTableName="leave_type"/>
        
        <addForeignKeyConstraint baseColumnNames="leave_id"
                                 baseTableName="leave_rule"
                                 constraintName="fk_leave_rule_leave_id"
                                 referencedColumnNames="id"
                                 referencedTableName="leave_type"/>

    </changeSet>
</databaseChangeLog>
