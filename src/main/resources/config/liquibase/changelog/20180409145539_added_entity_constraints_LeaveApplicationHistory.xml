<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity LeaveApplicationHistory.
    -->
    <changeSet id="20180409145539-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="employee_id"
                                 baseTableName="leave_application_history"
                                 constraintName="fk_leave_application_history_employee_id"
                                 referencedColumnNames="id"
                                 referencedTableName="employee"/>

        <addForeignKeyConstraint baseColumnNames="actor_id"
                                 baseTableName="leave_application_history"
                                 constraintName="fk_leave_application_history_actor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="employee"/>

        <addForeignKeyConstraint baseColumnNames="leave_type_id"
                                 baseTableName="leave_application_history"
                                 constraintName="fk_leave_application_history_leave_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="leave_type"/>

    </changeSet>
</databaseChangeLog>
